{
  "name": "a2a",
  "version": "0.5.0",
  "description": "Agent2Agent Protocol Implementation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "fix-import-extensions": "node scripts/codemod-add-js-extensions.js",
    "fix-dist-directory-imports": "node scripts/fix-dist-directory-imports.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "bump-version": "node scripts/bump-version.js",
    "prepare-npm": "npm run fix-import-extensions && npm run build && npm run fix-dist-directory-imports && node scripts/prepare-npm.js",
    "publish-npm": "npm run prepare-npm && cd tatou && npm publish",
    "link-local": "npm run prepare-npm && cd tatou && npm link && cd .. && echo '\nâœ… Package linked! You can now run \"npm link tatou\" in your test project\n'"
  },
  "dependencies": {
    "tatou": "^0.3.0",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/config-array": "^0.20.0",
    "@eslint/object-schema": "^2.1.6",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitest/coverage-v8": "^3.1.3",
    "eslint": "^9.27.0",
    "glob": "^11.0.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.3.3",
    "vitest": "^3.1.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
